{% for bool in BooleanTest.all() %}
    <pre>
        {{ bool }}
    </pre>
    {{ bool.checkbox }}
    {% if bool.checkbox %}
        Checked!
    {% else %}
        Not checked!
    {% endif %}
    <br>
    {{ bool.checkbox_optional }}
    {% if bool.checkbox_optional %}
        Checked!
    {% else %}
        Not checked!
    {% endif %}
    <hr>
{% endfor %}